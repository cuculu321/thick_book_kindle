{
  "name": "think book kindle",
  "description": "Amazonの本の厚さを表示します。",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.amazon.co.jp/*"],
      "css": ["book_think.css"],
      "js": ["display_think_of_book.js"]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
